    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "dtslint": "^3.4.1",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "knex": "^0.17.6",
    "mocha": "3.5.3",
    "mysql": "^2.17.1",
    "nyc": "^14.1.1",
    "objection": "^2.0.0",
    "pg": "^7.14.0",
    "prettier": "1.19.1",
    "sqlite3": "^4.1.0",
    "typescript": "^3.8.3"
