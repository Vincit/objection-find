{
  "name": "objection-find",
  "version": "2.1.0",
  "description": "Build search queries for objection.js models using HTTP query parameters.",
  "main": "objection-find.js",
  "types": "typings/index.d.ts",
  "license": "MIT",
  "scripts": {
    "lint:types": "dtslint --onlyTestTsNext typings",
    "test": "nyc mocha --slow 10 --timeout 7000 --reporter spec tests/test.js",
    "test-only": "mocha --slow 10 --timeout 50000 --reporter spec tests/test.js",
    "perf-test": "node tests/perf.js",
    "perf-test-init": "node tests/perf-init.js",
    "test:ci": "npm run lint && npm run test",
    "coveralls": "cat ./test-coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prettier": "prettier --write \"{lib,tests}/**/*.{js,ts}\"",
    "lint": "eslint --format codeframe \"lib/**/*.js\" \"tests/**/*.js\""
  },
  "author": {
    "name": "Sami KoskimÃ¤ki",
    "email": "sami.koskimaki@vincit.com",
    "url": "https://github.com/koskimas"
  },
  "contributors": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com",
      "url": "https://github.com/kibertoad"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/vincit/objection-find.git"
  },
  "keywords": [
    "objection",
    "sql",
    "query",
    "postgresql",
    "mysql",
    "sqlite3",
    "query parameter"
  ],
  "files": [
    "API.md",
    "README.md",
    "LICENSE",
    "objection-find.js",
    "typings/index.d.ts",
    "lib/*"
  ],
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/node": "^13.11.0",
    "babel-eslint": "^10.1.0",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.11",
    "dtslint": "^3.4.1",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "knex": "~0.20.13",
    "mocha": "5.2.0",
    "mysql": "^2.18.1",
    "nyc": "^14.1.1",
    "objection": "^1.6.11",
    "pg": "^7.14.0",
    "prettier": "^2.0.2",
    "sqlite3": "^4.1.1",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "objection": ">=1.6.11 <2.0.0"
  },
  "engines": {
    "node": ">=8"
  },
  "nyc": {
    "description": "test coverage",
    "reporter": [
      "lcov",
      "text"
    ],
    "report-dir": "./test-coverage"
  }
}
